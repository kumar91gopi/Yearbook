<div class="nested-fields">
    <div class= "input-field">
    <%=f.label :institute, "Institution Name"%>
    <%=f.text_field :institute%>
    </div>
    <div class= "input-field">
    <%=f.label :field, "Specialization"%>
    <%=f.text_field :field%>
    </div>
    <div class= "field">
    <%=f.label :from%>
    <%=f.date_select(:from, :discard_day => true, :start_year => 1995, :order => [:day, :month, :year], :include_blank => "Please select") %>
    </div>
    <div class= "field">
    <%=f.label :to%>
    <%=f.date_select(:to, :discard_day => true, :start_year => 1995, :order => [:day, :month, :year],  :include_blank => "Please select") %>
    </div>
     <%= link_to_remove_association "Remove ", f %>
</div>
