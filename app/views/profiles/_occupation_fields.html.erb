<div class="nested-fields">
    <div class= "input-field">
    <%=f.label :company, "Company"%>
    <%=f.text_field :company%>
    </div>
    <div class= "input-field">
    <%=f.label :position, "Position"%>
    <%=f.text_field :position%>
    </div>
    <div class= "field">
    <%=f.label :from%>
    <%= f.select :from, (1947..Time.now.year).to_a, :include_blank => "Select year" %>
    </div>
    <div class= "field">
    <%=f.label :to%>
    <%= f.select :to, (1947..Time.now.year).to_a, :include_blank => "Select year" %>
    </div>
    <div class= "input-field">
    <%=f.label :city, "City"%>
    <%=f.text_field :city%>
    </div>
        <%= link_to_remove_association "Remove ", f %>
    
     
    
</div>
